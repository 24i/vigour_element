{
  "name": "vigour-element",
  "description": "Reactive ui elements, blazing fast",
  "version": "1.0.0",
  "main": "lib/index.js",
  "scripts": {
    "test": "node test | tap-difflet",
    "watch": "nodemon test | tap-difflet",
    "cover": "istanbul cover --report none --print detail test/index.js",
    "view-cover": "istanbul report html && open ./coverage/index.html",
    "docs": "node_modules/vigour-doc/bin/vdoc",
    "example": "node example",
    "dev": "budo ./example/collection/index.js --css ./bundle.css --live -- -r ./package.json:package.json",
    "travis": "npm run cover -s && istanbul report lcov && ((cat coverage/lcov.info | coveralls) || exit 0)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/element.git"
  },
  "author": "vigour.io <dev@vigour.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vigour-io/element/issues"
  },
  "dependencies": {
    "html-element": "^1.5.0",
    "lodash.get": "^4.2.0",
    "lodash.set": "^4.1.0",
    "vigour-observable": "^2.1.4",
    "vigour-state": "^1.1.0",
    "vigour-util": "^2.0.8",
    "vigour-stamp": "^1.0.1",
    "vigour-ua": "^1.0.3"
  },
  "devDependencies": {
    "tape": "^4.4.0",
    "tap-difflet": "0.4.0",
    "nodemon": "^1.9.1",
    "coveralls": "^2.11.9",
    "istanbul": "^0.4.3",
    "vigour-doc": "^1.0.5",
    "budo": "^8.2.2",
    "postcssify": "^1.0.3",
    "babelify": "^7.3.0",
    "babel-preset-es2015": "^6.6.0"
  },
  "browser": {
    "html-element": false
  },
  "browserify": {
    "transform": [
      "postcssify",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "homepage": "https://github.com/vigour-io/element",
  "keywords": [
    "reactive",
    "state",
    "element",
    "DOM"
  ],
  "contributors": [
    "Youri Daamen <youri@vigour.io>",
    "Jim de Beer <jim@vigour.io>",
    "Nika Tomadze <nika@vigour.io>"
  ]
}
