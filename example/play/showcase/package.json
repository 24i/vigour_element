{
  "name": "vigour-element",
  "description": "DOM stuff for vigour-js",
  "version": "0.2.3",
  "main": "lib/index.js",
  "scripts": {
    "dev": "webpack-dev-server --history-api-fallback --inline --hot --content-base"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/element.git"
  },
  "author": "vigour.io <dev@vigour.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vigour-io/element/issues"
  },
  "browser": {
    "./lib/app/": "./lib/app/browser",
    "./lib/engine/ua/": "./lib/engine/ua/browser"
  },
  "dependencies": {
    "babel-core": "*",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "*",
    "css-loader": "*",
    "hyperquest": "^1.2.0",
    "less": "*",
    "less-loader": "*",
    "raw-loader": "*",
    "screenfull": "*",
    "style-loader": "*",
    "vigour-api": "0.0.3",
    "vigour-url": "vigour-io/vigour-url",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "vigour": {
    "api": {
      "video": {
        "getSource": {
          "url": "http://adm-ums.vigour.io/video/",
          "encodeJson": true,
          "withCredentials": false,
          "headers": {
            "Accept": "application/json"
          }
        },
        "sendPlayback": {
          "url": "http://adm-ums.vigour.io/video/",
          "httpMethod": "post",
          "encodeJson": true,
          "withCredentials": false,
          "headers": {
            "Accept": "application/json"
          }
        },
        "sendEvent": {
          "url": "http://adm-ums.vigour.io/video/event/",
          "httpMethod": "post",
          "encodeJson": true,
          "withCredentials": false,
          "headers": {
            "Accept": "application/json"
          }
        }
      }
    }
  },
  "devDependencies": {
    "json-loader": "^0.5.4"
  }
}
